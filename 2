 int a=10;
        int b=5;
        int suma = a+b;
        int resta= a-b;
        int multiplicar= a*b;
        int division= a/b;
        System.out.println("la suma da"+ a +"mas" +b+ "es igual a"+ suma);
         System.out.println("la resta da"+ a +"multipicado" + b + "es igual a" + multiplicar);
          System.out.println("la resta da"+ a +"resta"+b+"es igual a"+ resta);
           System.out.println("la division da"+ a +"dividido" + b+"es igual a"+ division);
